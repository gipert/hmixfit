{
    "id" : "l200a-taup",
    "logging" : "summary",
    "precision" : "kHigh",
    "output-dir" : "results",
    "n-bins": 300,
    "global-mode-search" : {
        "method" : "kOptMinuit"
    },
    "fit" : {
        "parameters" : {
            "2vbb" : {
                "range" : [3E4, 6E4],
                "long-name" : "2#nu#beta#beta",
                "units" : "cts"
            },
            "K42-homogeneous" : {
                "range" : [4E6, 7E6],
                "long-name" : "^{42}K - homogeneous in LAr",
                "units" : "cts"
            },
            "Bi212Tl208-cables" : {
                "range" : [1E4, 3E4],
                "long-name" : "^{212}Bi + ^{208}Tl - cables",
                "units" : "cts"
            },
            "Pb214Bi214-cables" : {
                "range" : [1E4, 3E4],
                "long-name" : "^{214}Pb + ^{214}Bi - cables",
                "units" : "cts"
            },
            "Ac228-pen-plates" : {
                "range" : [0, 2E4],
                "long-name" : "^{228}Ac - PEN plates",
                "units" : "cts"
            },
            "K40-hpge-support-copper" : {
                "range" : [0, 1E6],
                "long-name" : "^{40}K - HPGe support copper",
                "units" : "cts"
            },
            "K40-birds-nest" : {
                "range" : [0, 1E8],
                "long-name" : "^{40}K - bird's nest",
                "units" : "cts"
            },
            "Co60-hpge-support-copper" : {
                "range" : [0, 1E4],
                "long-name" : "^{60}Co - HPGe support copper",
                "units" : "cts"
            }
        },
        "theoretical-expectations" : {
            "inputs/data/l200a-taup-dataset.root" : {
                "raw/all" : {
                    "fit-range" : [500, 3000],
                    "rebin-factor" : 1,
                    "components" : [
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-hpge-bulk-2vbb-tier_pdf.root",
                            "components" : {"2vbb" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-lar-cylinder-K42-tier_pdf.root",
                            "components" : {"K42-homogeneous" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-hv-signal-cables-Ra224-to-Pb208-tier_pdf.root",
                            "components" : {"Bi212Tl208-cables" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-hv-signal-cables-Rn222-to-Pb214-tier_pdf.root",
                            "components" : {"Pb214Bi214-cables" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-pen-plates-Ra228-to-Ac228-tier_pdf.root",
                            "components" : {"Ac228-pen-plates" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-hpge-support-copper-K40-tier_pdf.root",
                            "components" : {"K40-hpge-support-copper" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-birds-nest-K40-tier_pdf.root",
                            "components" : {"K40-birds-nest" : {"hist-name" : "raw/all"}}
                        },
                        {
                            "root-file" : "inputs/pdfs/l200a-pdfs/l200a-hpge-support-copper-Co60-tier_pdf.root",
                            "components" : {"Co60-hpge-support-copper" : {"hist-name" : "raw/all"}}
                        }
                    ]
                }
            }
        }
    }
}
